	"main": "index.js",
	"scripts": {
		"webpack": "webpack",
		"debug": "node --inspect --inspect-brk ./node_modules/webpack/bin/webpack.js",
		"dev": "yarn webpack-dev-server --env.mode development --watch",
		"dev:preset:typescript": "yarn dev --env.presets typescript",
		"prod": "yarn webpack --env.mode production",
		"prod:preset:typescript": "yarn prod --env.presets typescript",
		"prod:preset:analyze": "yarn prod:preset:typescript --env.presets analyze",
		"prod:preset:compress": "yarn prod:preset:typescript --env.presets compress",
		"dev:debug": "yarn debug --env.mode development",
		"prod:debug": "yarn debug --env.mode production"
	},
